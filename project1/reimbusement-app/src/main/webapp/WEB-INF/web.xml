<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  	<servlet>
		<servlet-name>Front</servlet-name>
		<servlet-class>controller.FrontController.java</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Front</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Manager</servlet-name>
		<servlet-class>delegates.ManagerDelegate.java</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Manager</servlet-name>
		<url-pattern>/manager</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Employee</servlet-name>
		<servlet-class>delegates.EmployeeDelegate.java</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Employee</servlet-name>
		<url-pattern>/employee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>delegates.UserLoginDelegate.java</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>reimbursements</servlet-name>
		<servlet-class>delegates.ReimbursementsDelegate.java</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>reimbursements</servlet-name>
		<url-pattern>/reimbursements</url-pattern>
	</servlet-mapping>
</web-app>
